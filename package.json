{
  "name": "reusable-charts",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "dist/reusable-charts.es.js",
  "module": "dist/reusable-charts.es.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "prebuild:lib": "rm -rf lib/*",
    "build:lib": "babel --out-dir lib src",
    "prebuild:umd": "rm -rf dist/*",
    "prebuild:dist": "rm -rf dist/*",
    "build:dist": "rollup -c && rollup -c --environment PRODUCTION",
    "build:watch": "npm run build:lib -- --watch",
    "test": "mocha \"./src/**/*.test.js\" --require babel-core/register --timeout 5000",
    "test:watch": "npm run test -- --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "check": "flow check",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "lint-staged": "lint-staged",
    "dev": "node example/devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/once-ler/reusable-charts.git"
  },
  "keywords": [
    "d3.js",
    "dc.js",
    "reusable-charts"
  ],
  "author": "Henry Tao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/once-ler/reusable-charts/issues"
  },
  "homepage": "",
  "dependencies": {
    "fbjs": "^0.8.4",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chokidar": "^1.6.0",
    "d3": "^3.5.17",
    "dc": "^2.0.0-beta.32",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-flowtype-errors": "^1.3.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "flow-bin": "^0.34.0",
    "jsdom": "^9.8.0",
    "mocha": "^3.0.2",
    "mocha-jsdom": "^1.1.0",
    "node-watch": "^0.4.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0"
  }
}

{
  "name": "reusable-charts",
  "version": "0.0.3",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "dist/reusable-charts.es.js",
  "module": "dist/reusable-charts.min.js",
  "scripts": {
    "start2": "nodemon src/index.js --exec babel-node",
    "start": "webpack-dev-server --inline --hot --progress --colors --port 4000 --content-base example --config webpack.config.js",
    "build": "npm run build:lib && npm run build:dist",
    "prebuild:lib": "rm -rf lib/*",
    "build:lib": "babel --out-dir lib src",
    "prebuild:umd": "rm -rf dist/*",
    "prebuild:dist": "rm -rf dist/*",
    "build:dist": "cross-env NODE_ENV=production webpack src/index.js dist/reusable-charts.min.js",
    "build:watch": "npm run build:lib -- --watch",
    "test": "mocha \"./src/**/*.test.js\" --require babel-core/register --timeout 5000",
    "test:watch": "npm run test -- --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "check": "flow check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/once-ler/reusable-charts.git"
  },
  "keywords": [
    "d3.js",
    "dc.js",
    "reusable-charts"
  ],
  "author": "Henry Tao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/once-ler/reusable-charts/issues"
  },
  "homepage": "",
  "dependencies": {
    "colorbrewer": "^1.0.0",
    "crossfilter2": "^1.3.14",
    "d3": "^3.5.17",
    "dc": "^2.0.2",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "flow-bin": "^0.34.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.0.2",
    "node-watch": "^0.4.1",
    "nodemon": "^1.11.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
